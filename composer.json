{
  "name": "madewithlove/laravel-cqrs-es",
  "description": "A Laravel package to kick start CQRS and ES projects using Broadway and Tactician.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "broadway",
    "event sourcing",
    "ES",
    "CQRS",
    "Domain Driven Design",
    "DDD"
  ],
  "authors": [
    {
      "name": "Madewithlove",
      "email": "heroes@madewithlove.be"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "doctrine/dbal": "^2.5",
    "broadway/broadway": "^0.10",
    "illuminate/console": "^5.4",
    "illuminate/database": "^5.4",
    "illuminate/events": "^5.4",
    "illuminate/support": "^5.4",
    "matthiasnoback/broadway-serialization": "^1.0",
    "mattketmo/uuid-2x-bridge": "^1.0"
  },
  "require-dev": {
    "elasticsearch/elasticsearch": "^2.1",
    "laravel/framework": "^5.4",
    "fabpot/php-cs-fixer": "2.0.*@dev",
    "phpunit/phpunit": "^4.7",
    "alcaeus/mongo-php-adapter": "^1.0",
    "doctrine/mongodb": "^1.4",
    "madewithlove/elasticsearcher": "^0.4.1"
  },
  "autoload": {
    "psr-4": {
      "Madewithlove\\LaravelCqrsEs\\": "src/"
    }
  },
  "suggest": {
    "madewithlove/tactician-laravel": "Laravel wrapper for a configurable command bus"
  },
  "scripts": {
    "test": ""
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
